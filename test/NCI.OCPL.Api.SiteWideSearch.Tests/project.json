        "debugType": "full"
    },
    "tooling": {
        "defaultNamespace": "NCI.OCPL.Api.SiteWideSearch.Tests"
    },
    "tools": {
        "Microsoft.DotNet.Watcher.Tools": "1.0.0-preview2-final"
    }
